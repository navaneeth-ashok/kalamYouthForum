@model IEnumerable<KalamYouthForumWebApp.Models.SHGMember>

@{
    ViewData["Title"] = "View All SHG Members";
}

<h1>View All SHG Members</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dob)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BloodGroup)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.Encode("SHG ID")
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.SHEModel.SHEName)
            </th>*@
            @*<th></th>*@
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="ViewSHGMember" asp-route-id="@item.SHGMemberId">@Html.DisplayFor(modelItem => item.Name)</a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dob)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BloodGroup)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.SHEId">@Html.DisplayFor(modelItem => item.SHEId)</a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SHEModel.SHEName)
            </td>
            @*<td>
                <a asp-action="Edit" asp-route-id="@item.SHGMemberId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SHGMemberId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SHGMemberId">Delete</a>
            </td>*@
        </tr>
}
    </tbody>
</table>
