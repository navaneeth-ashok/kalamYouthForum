<form asp-action="UploadImageDB"
      asp-controller="Images"
      method="post"
      enctype="multipart/form-data">
    <input type="file" id="file1"
           name="file1"
           multiple="multiple" class="btn btn-info"/>
    <input type="hidden" id="projectID" name="projectID" value=@Model.Project.ProjectIDKey>
    <button type="submit" class="btn btn-primary">Upload File(s)</button>
    
</form>